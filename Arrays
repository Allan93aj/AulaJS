"CASAS"         [ 0    1    2    3...] 
const valores = [7.7, 8.9, 6.3, 9.2] 
console.log(valores[0], valores[3]) // 7.7  9.2 
console.log(valores[4]) // undefined

valores[4] = 10  // declara mais um array
console.log(valores)  // mostra todos os arrays
console.log(valores.length) // quantidade de arrays

            (objeto, boolean, nulo, string)
valores.push({id: 3}, false, null, 'teste') // adiciona mais elementos
console.log(valores)

console.log(valores.pop()) //retira o ultimo elemento
delete valores[0] // deleta 
console.log(valores)

console.log(typeof valores)
